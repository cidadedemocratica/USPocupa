            </ul>
          </td>
          <td style="padding-left:40px">
          <h3>Participe</h3>
            <ul>
              <li>
                <a href="/">Home</a>
              </li>
              <li>
                <%= link_to "Cadastre-se", cadastrar_url %>
              </li>
              <li>
                <%= link_to("Termos de uso", :controller => "home", :action => "termos_de_uso") %>
              </li>
              <li>
                <%= link_to("Manual de Boas Práticas", :controller => "home", :action => "boas_praticas") %>
              </li>
              <li>
                <%= link_to("Ajuda", :controller => "home", :action => "ajuda") %>
